<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- outer circle -->
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" >
            <solid android:color="#00fdead4" />

            <solid
                android:color="#00fdead4" />
            <stroke
                android:width="9dp"
                android:color="#ffffff" />
            <size android:width="68dp"
                android:height="68dp" />
        </shape>
    </item>

    <item
        android:bottom="9dp"
        android:left="9dp"
        android:right="9dp"
        android:top="9dp">
        <shape android:shape="oval" >
            <stroke
                android:width="1dp"
                android:color="#00000000" />
        </shape>
    </item>

</layer-list>